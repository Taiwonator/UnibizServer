generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  token String?
  firstName String?
  lastName String?
}

model experience {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  astroid String
  name    String
}

model society {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}

model union {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}
